library(rayshader)
library(rayrender)
coral <- rayrender::obj_model("Blender/hydno3d.obj", texture = TRUE)
obj_model
generate_ground(material = diffuse(checkercolor = "grey50")) %>%
add_object(coral) %>%
add_object(sphere(x = 0, y = 10, z = 0, radius = 5, material = light(intensity = 100))) %>%
render_scene(parallel = TRUE, samples = 200,
